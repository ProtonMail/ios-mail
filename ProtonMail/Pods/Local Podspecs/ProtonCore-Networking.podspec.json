{
  "name": "ProtonCore-Networking",
  "version": "2.4.0",
  "summary": "shared frameworks",
  "description": "ios shared frameworks for all client apps",
  "homepage": "https://github.com/ProtonMail",
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "zhj4478": "feng@pm.me"
  },
  "source": {
    "git": "git@gitlab.protontech.ch:apple/shared/protoncore.git",
    "tag": "2.4.0"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.12.1"
  },
  "swift_versions": [
    "5.1"
  ],
  "source_files": "libraries/Networking/Sources/Networking/**/*",
  "prepare_command": "bash libraries/Networking/Tests/Networking/Scripts/prepare_obfuscated_constants.sh",
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "default_subspecs": "AFNetworking",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "preserve_paths": "libraries/Networking/Tests/Networking/Scripts/*",
      "script_phases": {
        "name": "Obfuscation",
        "script": "${PODS_TARGET_SRCROOT}/libraries/Networking/Tests/Networking/Scripts/prepare_obfuscated_constants.sh",
        "execution_position": "before_compile",
        "output_files": [
          "${PODS_TARGET_SRCROOT}/libraries/Networking/Tests/Networking/ObfuscatedConstants.swift"
        ]
      },
      "source_files": "libraries/Networking/Tests/Networking/*.swift",
      "dependencies": {
        "ProtonCore-TestingToolkit/UnitTests/Networking": [
          "2.4.0"
        ],
        "ProtonCore-Doh": [
          "2.4.0"
        ],
        "ProtonCore-Services": [
          "2.4.0"
        ],
        "ProtonCore-APIClient": [
          "2.4.0"
        ],
        "OHHTTPStubs/Swift": [

        ],
        "TrustKit": [

        ]
      }
    }
  ],
  "subspecs": [
    {
      "name": "AFNetworking",
      "dependencies": {
        "AFNetworking": [
          "~> 4.0"
        ],
        "TrustKit": [

        ]
      }
    },
    {
      "name": "Alamofire",
      "dependencies": {
        "Alamofire": [
          "~> 5.2"
        ],
        "TrustKit": [

        ]
      }
    }
  ],
  "swift_version": "5.1"
}
