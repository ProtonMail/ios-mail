{
  "name": "ProtonCore-UIFoundations",
  "version": "2.4.0",
  "summary": "shared frameworks",
  "description": "ios shared frameworks for all client apps",
  "homepage": "https://github.com/ProtonMail",
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "authors": {
    "zhj4478": "feng@pm.me"
  },
  "source": {
    "git": "git@gitlab.protontech.ch:apple/shared/protoncore.git",
    "tag": "2.4.0"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.12.1"
  },
  "swift_versions": [
    "5.1"
  ],
  "dependencies": {
    "ProtonCore-Log": [
      "2.4.0"
    ],
    "ProtonCore-CoreTranslation": [
      "2.4.0"
    ],
    "ProtonCore-Foundations": [
      "2.4.0"
    ]
  },
  "ios": {
    "source_files": "libraries/UIFoundations/Sources/**/*.swift",
    "resource_bundles": {
      "PMUIFoundations": [
        "libraries/UIFoundations/Sources/Assets.xcassets",
        "libraries/UIFoundations/Sources/**/*.{xib,storyboard,geojson}"
      ]
    }
  },
  "osx": {
    "source_files": [
      "libraries/UIFoundations/Sources/PMUIFoundations.swift",
      "libraries/UIFoundations/Sources/Colors/Colors.swift",
      "libraries/UIFoundations/Sources/Colors/Color+Extestion.swift",
      "libraries/UIFoundations/Sources/Colors/NewColorScheme/ColorManager.swift",
      "libraries/UIFoundations/Sources/Colors/NewColorScheme/NewColors.swift",
      "libraries/UIFoundations/Sources/Extension/NSColor+Helper.swift"
    ],
    "resource_bundles": {
      "PMUIFoundations": [
        "libraries/UIFoundations/Sources/Assets.xcassets"
      ]
    }
  },
  "pod_target_xcconfig": {
    "APPLICATION_EXTENSION_API_ONLY": "NO"
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "libraries/UIFoundations/Tests/**/*"
    }
  ],
  "swift_version": "5.1"
}
